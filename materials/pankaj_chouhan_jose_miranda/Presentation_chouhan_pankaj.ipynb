   "source": [
    "# function depend on Three input param, beta, x, y. Variable is only beta\n",
    "function sqerror(betas, X, Y)\n",
    "    err = 0.0\n",
    "    for i in 1:length(X)\n",
    "        pred_i = betas[1] + betas[2] * X[i]\n",
    "        err += (Y[i] - pred_i)^2\n",
    "    end\n",
    "    return err\n",
    "end"
   ]
